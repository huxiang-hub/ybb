<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.anaysis.mysqlmapper.BaseDeptInfoMapper">
    <select id="getByerpId" resultType="com.anaysis.entity.BaseDeptInfo" >
        SELECT * FROM yb_base_deptinfo WHERE erp_id = #{erpId}
    </select>

    <select id="getByDpNum" resultType="java.lang.Integer" >
        SELECT id FROM yb_base_deptinfo WHERE dp_num = #{getByDpNum}
    </select>
</mapper>
